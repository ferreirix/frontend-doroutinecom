{
  "name": "gym-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path ./src/styles --include-path ./src/components",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./src/styles --include-path ./src/components",
    "start-js": "react-scripts start",
    "start-flow": "npm run flow check",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "ENV=development jest",
    "test-production": "ENV=production jest",
    "eject": "react-scripts eject",
    "flow": "flow",
    "deploy": "npm-run-all -s build sync",
    "sync": "ansible-playbook -i ../deployment/hosts ../deployment/site.yml -t sync.app"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chart.js": "^2.7.1",
    "classnames": "^2.2.5",
    "formik": "^0.10.5",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-range": "^3.0.3",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-animate-on-change": "^1.0.1",
    "react-beautiful-dnd": "^3.0.0",
    "react-dates": "^16.0.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tabs": "^2.2.1",
    "react-textarea-autosize": "^5.2.1",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "seamless-immutable": "^7.1.2",
    "superstruct": "^0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.6",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.0",
    "faker": "^4.1.0",
    "jest": "^22.0.6",
    "jest-cli": "^22.0.6",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^1.0.0",
    "react-scripts": "1.1.0",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "why-did-you-update": "^0.1.0"
  }
}
