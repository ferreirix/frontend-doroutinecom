{
  "name": "gym-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/ --include-path ./src/styles --include-path ./src/components",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path ./src/styles --include-path ./src/components",
    "start-js": "react-scripts-ts start",
    "start-flow": "npm run flow check",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts-ts build",
    "build": "npm-run-all build-css build-js",
    "test": "NODE_ENV=development jest",
    "test-production": "NODE_ENV=production jest",
    "eject": "react-scripts-ts eject",
    "flow": "flow",
    "deploy": "npm-run-all -s build sync",
    "sync": "ansible-playbook -i ../deployment/hosts ../deployment/site.yml -t sync.app"
  },
  "dependencies": {
    "@types/chart.js": "^2.7.10",
    "@types/classnames": "^2.2.3",
    "@types/helmet": "^0.0.37",
